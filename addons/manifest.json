{
  "manifest_version": 2,
  "name": "firefox-extension-template",
  "description": "firefox-extension-template",
  "version": "2024.1.13.1",
  "default_locale": "en",
  "browser_specific_settings": {
    "gecko": {
      "id": "firefox-extension-template@example.com"
    }
  },
  "icons": {
    "48": "icons/48.png"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "webNavigation",
    "scripting",
    "clipboardWrite",
    "clipboardRead",
    "webRequest",
    "webRequestBlocking",
    "menus",
    "notifications",
    "storage",
    "unlimitedStorage"
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icons/48.png",
    "default_title": "setting",
    "default_popup": "popup-browser-act.html"
  }
}